<div class="row">
	<div class="col-md-9">
		<div class="page-header">
		  <h1>New Rental Booking Instruction</h1>
		</div>
		<%= form_for([:admin, @rental_booking_instruction],:html => {:role => 'form', :class => "formFor"}) do |f| %>
			<%= render partial: 'form', locals: {f:f} %>
			<hr />
			<%= button_tag(type: 'submit', class:"btn  btn-outline-secondary btn-med") do %>
				<i class="icon-ok"></i>
				Create
			<% end %>
			<%= link_to(raw('<button type="button" class="btn  btn-outline-secondary btn-med" style="float:right;">
				<i class="icon-cancel"></i>
		   	 	Cancel</button>'), admin_rental_path(@rental)) %>
		<% end %>
	</div>
	<div class="col-md-3">
		<%= operations_menu(@rental_booking_instruction,[@rental]) %>
	</div>
</div>
