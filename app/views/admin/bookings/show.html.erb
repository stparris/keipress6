<div class="row">
	<div class="col-md-9">
		<div class="page-header">
			<h1><%= @booking.name %></h1>
		</div>	
		<%= object_table(@booking) %>
		<%= link_to edit_admin_booking_path(@booking) do %>
			<button type="submit" class="btn  btn-outline-secondary"><i class="icon-pencil"></i> Edit</button>
		<% end %>
		&nbsp;&nbsp;<%= link_to admin_booking_path(@booking), method: :delete, data: { confirm: "Delete booking?" } do %>
			<button type="submit" class="btn btn-danger"><i class="icon-trash"></i> Delete</button>
		<% end %>
	</div>
	<div class="col-md-3">
		<%-# operations_menu(@booking,[]) %>
	</div>
</div>
