<div class="row">
	<div class="col-md-9">
		<div class="page-header">
		  <h1>Edit Video: <%= @medium.name %></h1>
		</div>
		<%= form_for([:admin, @medium],:html => {:role => 'form', :class => "formFor"}) do |f| %>
			<%= render partial: 'form', locals: {f:f} %>

	</div>
	<div class="col-md-3">
		<%= operations_menu(@medium,[]) %>
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		<%= render partial: 'admin/images/image_select_table', locals: {f: f,content: @medium } %>
		<hr />
		<%= button_tag(type: 'submit', class:"btn  btn-outline-secondary btn-med") do %>
			<i class="icon-ok"></i>
			Save
		<% end %>&nbsp;&nbsp;
		<%= link_to(raw('<button type="button" class="btn  btn-outline-secondary btn-med">
			<i class="icon-cancel"></i>
	   	 	Cancel</button>'), admin_medium_path(@medium)) %>
		<% end %>
	</div>
</div>
