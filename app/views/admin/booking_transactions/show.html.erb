<div class="row">
	<div class="col-md-9">
		<div class="page-header">
			<h1><%= @booking_transaction.name %></h1>
		</div>	
		<%= object_table(@booking_transaction) %>
		<%= link_to edit_admin_booking_transaction_path(@booking_transaction) do %>
			<button type="submit" class="btn  btn-outline-secondary"><i class="icon-pencil"></i> Edit</button>
		<% end %>
		&nbsp;&nbsp;<%= link_to admin_booking_transaction_path(@booking_transaction), method: :delete, data: { confirm: "Delete booking_transaction?" } do %>
			<button type="submit" class="btn btn-danger"><i class="icon-trash"></i> Delete</button>
		<% end %>
	</div>
	<div class="col-md-3">
		<%= operations_menu(@booking_transaction) %>
	</div>
</div>
